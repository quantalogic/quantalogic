{
    "mcp_servers": {
        "sqlite": {
            "transport_type": "stdio",
            "command": "docker",
            "args": [
                "run",
                "--rm",
                "-i",
                "-v",
                "mcp-test:/mcp",
                "mcp/sqlite",
                "--db-path",
                "/mcp/test.db"
            ],
            "env": {},
            "auth": {
                "type": "none"
            }
        }
    },
    "resources": [
        {
            "id": "test_db",
            "uri": "sqlite:///mcp/test.db"
        }
    ],
    "prompts": [
        {
            "name": "query_info",
            "text": "Provide database schema info"
        }
    ],
    "tools": [
        {
            "name": "sql_query",
            "description": "Execute a SQL query on the SQLite database",
            "implementation": {
                "type": "mcp_tool",
                "server": "sqlite",
                "tool_name": "query"
            }
        }
    ]
}